<div id="inventory">
    <div class="row">
        {{#each items}}
            <div class="inventoryItem col-md-2 {{this.quality}}" id="inventoryItem{{@index}}">
                <h5>
                    {{this.name}}
                </h5>
                <p>{{this.exterior}}</p>
                    <img src="{{this.icon}}" alt="{{this.name}}">
                <p>
                    <a class="inspect" href="{{this.inspectLink}}">Inspect</a> <span>&nbsp;</span> <a class="inspect" href="{{this.marketlink}}">Market</a>
                </p>
                <p class="nametag">
                    {{this.nametag}}
                </p>
                <p class = "tradability">
                    {{this.tradability}}
                </p>
                <div class="countdown" data-countdown="{{this.tradability}}"></div>
            </div>
        {{/each}}
    </div>
</div>